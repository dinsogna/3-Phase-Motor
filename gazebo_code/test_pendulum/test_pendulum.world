<sdf version='1.6'>
  <world name="default">

    <!-- Ground Plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>
  <model name='test_pendulum'>
    <allow_auto_disable>0</allow_auto_disable>
    <link name='base_link'>
      <pose frame=''>0 0 0 0 -0 0</pose>
      <inertial>
        <pose frame=''>0 0 0.5 0 -0 0</pose>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.08416666666666667</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.08416666666666667</iyy>
          <iyz>0</iyz>
          <izz>0.001666666666666667</izz>
        </inertia>
      </inertial>
      <collision name='base_link_collision'>
        <pose frame=''>0 0 0.5 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 1.0</size>
          </box>
        </geometry>
      </collision>
      <visual name='base_link_visual'>
        <pose frame=''>0 0 0.5 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 1.0</size>
          </box>
        </geometry>
      </visual>
      <visual name='base_link_to_first_link_joint_visual'>
        <pose frame=''>0.05 0 1.0 0 1.57 0</pose>
        <geometry>
          <cylinder>
            <radius>0.05</radius>
            <length>0.2</length>
          </cylinder>
        </geometry>
      </visual>
    </link>
    <link name='first_link'>
      <pose frame=''>0.1 0 1.0 0 0 0</pose>
      <inertial>
        <pose frame=''>0 0 0.25 0 -0 0</pose>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.021666666666666667</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.021666666666666667</iyy>
          <iyz>0</iyz>
          <izz>0.001666666666666667</izz>
        </inertia>
      </inertial>
      <collision name='first_link_collision'>
        <pose frame=''>0 0 0.25 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.5</size>
          </box>
        </geometry>
      </collision>
      <visual name='first_link_visual'>
        <pose frame=''>0 0 0.25 0 0 0</pose>
        <geometry>
          <box>
            <size>0.1 0.1 0.5</size>
          </box>
        </geometry>
      </visual>
    </link>
    <joint name='J01' type='revolute'>
      <child>first_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <effort>52.8</effort>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <provide_feedback>true</provide_feedback> 
        </ode>
      </physics>
    </joint>
    <joint type="fixed" name="fixed base_link to ground">
      <parent>world</parent>
      <child>base_link</child>
    </joint>
    <plugin name="test_pendulum" filename="libtest_pendulum.so"/>
  </model>
  </world>
</sdf>
